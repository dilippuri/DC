gcd_p_1(char *host, int x, int y)
{
	CLIENT *clnt;
	gcdout  *result_1;
	gcdin  gcd_1_arg;

#ifndef	DEBUG
	clnt = clnt_create (host, GCD_P, GCD_V, "udp");
	if (clnt == NULL) {
		clnt_pcreateerror (host);
		exit (1);
	}
#endif	/* DEBUG */
	gcd_1_arg.x = x;
	gcd_1_arg.y = y;
	result_1 = gcd_1(&gcd_1_arg, clnt);
	if (result_1 == (gcdout *) NULL) {
		clnt_perror (clnt, "call failed");
	}
	else{
		printf("GCD(%d, %d) is : %d",x,y,result_1->z);
	}
#ifndef	DEBUG
	clnt_destroy (clnt);
#endif	 /* DEBUG */
}


